{
  "name": "lune-react-native",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant=devDebug --appId com.lunereactnative.dev",
    "android-clean": "cd android && ./gradlew clean",
    "android-build": "cd android && ./gradlew assembleQaDebug",
    "ios": "react-native run-ios",
